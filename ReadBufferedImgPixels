BufferedImage img = ImageIO.read(file);
        for(int i=0; i<1; i++){
            for(int j =0;j<img.getWidth();j++){
                Color c = new Color(img.getRGB(j, i));
                int R = c.getRed();
                int G = c.getBlue();
                int B = c.getBlue();
                int A = c.getAlpha();
                System.out.println("ARGB:"+A+";"+R+";"+G+";"+B);
            }
        }
